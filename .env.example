# Variables de entorno para configuracion segura
# NO SUBIR ESTE ARCHIVO A GITHUB - Agregar a .gitignore

# Puerto del servidor
PORT=3000

# Secreto para encriptar sesiones (cambiar en produccion)
SESSION_SECRET=tu-clave-super-secreta-cambiala-en-produccion-123456789

# Entorno de ejecucion
NODE_ENV=development

# Duracion de la cookie de sesion en milisegundos (24 horas)
COOKIE_MAX_AGE=86400000
